{
  "name": "marlinraker",
  "version": "0.1.0",
  "main": "src/Server.ts",
  "scripts": {
    "postinstall": "husky install",
    "dev": "cross-env NODE_ENV=development MARLINRAKER_DIR=./marlinraker_files/ ts-node src/Server.ts --serve-static",
    "build": "npm run lint && webpack && node scripts/after_build.js",
    "build.zip": "npm run build && node scripts/zip.js",
    "lint": "eslint src --max-warnings=0 --fix"
  },
  "author": "pauhull",
  "license": "GPLv3",
  "dependencies": {
    "chalk": "^4.1.2",
    "express": "^4.18.1",
    "fs-extra": "^10.1.0",
    "logrotate-stream": "^0.2.8",
    "multer": "^1.4.5-lts.1",
    "node-json-db": "^2.1.0",
    "rfc4648": "^1.5.2",
    "serialport": "^10.4.0",
    "source-map-support": "^0.5.21",
    "systeminformation": "^5.12.4",
    "tslib": "^2.4.0",
    "unzipper": "^0.10.11",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/logrotate-stream": "^0.2.31",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.9",
    "@types/source-map-support": "^0.5.6",
    "@types/unzipper": "^0.10.5",
    "@types/webpack-node-externals": "^2.5.3",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0",
    "zip-local": "^0.3.5"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.6",
    "utf-8-validate": "^5.0.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --max-warnings=0 --fix",
      "git add"
    ]
  }
}
